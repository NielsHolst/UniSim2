Simulation sim {
  .iterations = 36
  .steps = 365
  Sequence latitude {
    .min = 29
    .max = 65
    .by  = "reset"
  }
  Calendar calendar {
    .latitude = latitude[value]
  }
  OutputR {
    .end = "day_length.R"
    OutputText {
      .ports = (calendar[latitude] calendar[dayLength])
    }
  }
}
