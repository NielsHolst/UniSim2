// phys-dev-6.box
Simulation sim {
  .steps = 60
  Stage distDelay {
    .initial = 100 
    .duration = 30
    .growthFactor = 2
    .k = 25
    .timeStep = 1
  }
  OutputR {
    PageR {
      PlotR {
        .layout = "facetted"
        .ports = distDelay[outflow]|distDelay[content]|distDelay[outflowTotal]|distDelay[growth]
        .ggplot = "geom_vline(xintercept=30, linetype='longdash') + geom_point() + scale_x_continuous(breaks=10*(0:6))"
      }
    }
  }
}

