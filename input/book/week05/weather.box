Simulation sim {
  .steps = 365
  Calendar calendar {
    .initialDateTime = 1/1/2016
  }
  student::Weather weather {
    .dayOfYear = calendar[dayOfYear]
  }
  OutputR {
    PageR {
      PlotR {
        .ports = (weather[T])
      }
      PlotR {
        .ports = (calendar[dayOfYear])
      }
    }
  }
}