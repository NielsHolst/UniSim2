// book/photo-thermal3.box
Simulation sim {
  .steps = 365
  Calendar calendar {
    .initialDateTime = 1/1/2016
  }
  DayLengthScale light {
    .dayLength = calendar[dayLength]
  }
  OutputR {
    PageR {
      .xAxis = calendar[date]
      PlotR {
        .ports = light[value]
      }
    }
  }
}