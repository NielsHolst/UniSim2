Simulation butterfly( steps = 180 )
{
	Calendar calendar( initialDate = weather[firstDateTime] )
	Records weather( fileName = "weather/flakkebjerg 2009.txt" )
	Stage egg ( initial = 100
	            duration = 140
			        dt = ./time[step] )
  {
		  DayDegrees time( T0 = 8.3
				               T = weather[Tavg] )
	}
	OutputR ( xAxis = calendar[date]
		        overlay = FALSE )
}
