					<port name="leakage" externalName="leakage" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="1"/>
				</box>
			</box>
			<box class="vg::EnergyFluxSum" name="energyFlux">
				<box class="vg::EnergyFluxFloor" name="floor">
					<port name="Uindoors" externalName="floor-Uindoors" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="7.5"/>
					<port name="Usoil" externalName="floor-Usoil" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="4"/>
					<port name="heatCapacity" externalName="floor-heatCapacity" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="1000"/>
					<port name="emissivity" externalName="floor-emissivity" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="0.85"/>
					<box class="vg::FloorRadiationAbsorbed" name="radiationAbsorbed">
						<port name="reflectance" externalName="floor-reflectance" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="0.5"/>
					</box>
				</box>
			</box>
		</box>
	</box>
