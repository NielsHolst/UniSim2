				<port name="parPhotonCoef" externalName="MicromolParPerWatt" source="/DVV_SETUP/Greenhouse/Lamps/Lamp/Constants/Parameters/Value" value="1.611"/>
				<port name="age" externalName="UsedHours" source="/DVV_SETUP/Greenhouse/Lamps/Lamp/Constants/Parameters/Value" value="0"/>
				<port name="lifeTime" externalName="LifeTime" source="/DVV_SETUP/Greenhouse/Lamps/Lamp/Constants/Parameters/Value" value="12000"/>
			</box>
		</box>
		<box class="ProportionalSignal" name="heating">
			<port name="signalReset" ref="indoors/temperature[value]"/>
			<port name="input" ref="controllers/heating[value]"/>
