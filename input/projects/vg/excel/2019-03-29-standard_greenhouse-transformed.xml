			<port name="spanWidth" externalName="SpanSize" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="4"/>
			<port name="length" externalName="Length" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="100"/>
			<port name="height" externalName="TrempelHeight" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="4"/>
			<port name="roofPitch" externalName="RoofPitch" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="26"/>
			<port name="reflection" externalName="GreenhouseReductionFactorLight" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="0.2"/>
		</box>
		<box class="vg::Shelter" name="shelter">
			<box class="vg::ShelterFace" name="roof1">
				<box class="vg::Cover" name="cover">
					<port name="transmissivity" externalName="PaneTransmission" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="0.0089"/>
					<port name="U4" externalName="PaneUValue" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="6.5"/>
				</box>
				<box class="vg::Screens" name="screens"/>
			</box>
			<box class="vg::ShelterFace" name="roof2">
				<box class="vg::Cover" name="cover">
					<port name="transmissivity" externalName="PaneTransmission" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="0.0089"/>
					<port name="U4" externalName="PaneUValue" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="6.5"/>
				</box>
				<box class="vg::Screens" name="screens"/>
			</box>
			<box class="vg::ShelterFace" name="side1">
				<box class="vg::Cover" name="cover">
					<port name="transmissivity" externalName="PaneTransmission" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="0.0089"/>
					<port name="U4" externalName="PaneUValue" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="6.5"/>
				</box>
				<box class="vg::Screens" name="screens">
					<box class="vg::Screen" name="energy">
						<port name="orientation" externalName="No name" source="Fixed" value="cover"/>
						<port name="transmissivityLight" externalName="Transmission" source="/DVV_SETUP/Greenhouse/Screens/Screen/Constants/Parameters/Value" value="0.007"/>
						<port name="energyLossReduction" externalName="InsulationRatio" source="/DVV_SETUP/Greenhouse/Screens/Screen/Constants/Parameters/Value" value="0.0045"/>
						<port name="transmissivityAir" externalName="Porosity" source="/DVV_SETUP/Greenhouse/Screens/Screen/Constants/Parameters/Value" value="0.0003"/>
						<port name="state" ref="actuators/screens/energy[value]"/>
					</box>
				</box>
			</box>
			<box class="vg::ShelterFace" name="side2">
				<box class="vg::Cover" name="cover">
					<port name="transmissivity" externalName="PaneTransmission" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="0.0089"/>
					<port name="U4" externalName="PaneUValue" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="6.5"/>
				</box>
				<box class="vg::Screens" name="screens">
					<box class="vg::Screen" name="energy">
						<port name="orientation" externalName="No name" source="Fixed" value="cover"/>
						<port name="transmissivityLight" externalName="Transmission" source="/DVV_SETUP/Greenhouse/Screens/Screen/Constants/Parameters/Value" value="0.007"/>
						<port name="energyLossReduction" externalName="InsulationRatio" source="/DVV_SETUP/Greenhouse/Screens/Screen/Constants/Parameters/Value" value="0.0045"/>
						<port name="transmissivityAir" externalName="Porosity" source="/DVV_SETUP/Greenhouse/Screens/Screen/Constants/Parameters/Value" value="0.0003"/>
						<port name="state" ref="actuators/screens/energy[value]"/>
					</box>
				</box>
			</box>
			<box class="vg::ShelterFace" name="end1">
				<box class="vg::Cover" name="cover">
					<port name="transmissivity" externalName="PaneTransmission" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="0.0089"/>
					<port name="U4" externalName="PaneUValue" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="6.5"/>
				</box>
				<box class="vg::Screens" name="screens"/>
			</box>
			<box class="vg::ShelterFace" name="end2">
				<box class="vg::Cover" name="cover">
					<port name="transmissivity" externalName="PaneTransmission" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="0.0089"/>
					<port name="U4" externalName="PaneUValue" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="6.5"/>
				</box>
				<box class="vg::Screens" name="screens"/>
			</box>
		</box>
	</box>
	<!-- *** Indoors *** -->
	<box class="vg::Indoors" name="indoors">
		<box class="vg::Given" name="given">
			<box class="vg::AirFluxGiven" name="airFlux">
