				<port name="endDate" externalName="Cli_MaxVentDecHighRH" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/ToDay" value="31-12-2019"/>
				<port name="beginTime" externalName="Cli_MaxVentDecHighRH" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/FromTime" value="18:00"/>
				<port name="endTime" externalName="Cli_MaxVentDecHighRH" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/ToTime" value="23:59"/>
				<port name="signalInside" externalName="Cli_MaxVentDecHighRH" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/Value" value="3.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="ventilationThresholdBand">
