			</box>
		</box>
		<box class="PrioritySignal" name="co2Capacity">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="Cli_CO2Capacity" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="10.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="co2Setpoint">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="Cli_CO2Setpoint" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="900.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="co2VentilationThreshold">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="Cli_CO2VentilationThreshold" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="0.1"/>
			</box>
		</box>
		<box class="PrioritySignal" name="co2VentilationBand">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="Cli_CO2VentilationBand" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="0.2"/>
			</box>
		</box>
		<box class="PrioritySignal" name="chalk">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="Cli_ShadingAgentReduction" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="0.0"/>
			</box>
		</box>
		<box name="growthLights">
			<box name="growthLight1">
				<box class="PrioritySignal" name="thresholdLow">
					<port name="reverseOrder" value="TRUE"/>
					<box name="default">
						<newPort name="flagIsUp" value="TRUE"/>
						<newPort name="signal" externalName="AssLightOn1" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="40.0"/>
					</box>
				</box>
				<box class="PrioritySignal" name="thresholdHigh">
					<port name="reverseOrder" value="TRUE"/>
					<box name="default">
						<newPort name="flagIsUp" value="TRUE"/>
						<newPort name="signal" externalName="AssLightOff1" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="600.0"/>
					</box>
				</box>
				<box class="PrioritySignal" name="setting">
					<port name="reverseOrder" value="TRUE"/>
					<box name="default">
						<newPort name="flagIsUp" value="TRUE"/>
						<newPort name="signal" externalName="AssLightActive1" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="1.0"/>
					</box>
					<box class="DateTimeSignal">
						<port name="beginDate" externalName="AssLightActive1" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/FromDay" value="01-01-2019"/>
						<port name="endDate" externalName="AssLightActive1" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/ToDay" value="31-12-2019"/>
						<port name="beginTime" externalName="AssLightActive1" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/FromTime" value="18:00"/>
						<port name="endTime" externalName="AssLightActive1" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/ToTime" value="23:59"/>
						<port name="signalInside" externalName="AssLightActive1" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/Value" value="0.0"/>
					</box>
				</box>
			</box>
			<box name="growthLight2">
				<box class="PrioritySignal" name="thresholdLow">
					<port name="reverseOrder" value="TRUE"/>
					<box name="default">
						<newPort name="flagIsUp" value="TRUE"/>
						<newPort name="signal" externalName="AssLightOn2" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="40.0"/>
					</box>
				</box>
				<box class="PrioritySignal" name="thresholdHigh">
					<port name="reverseOrder" value="TRUE"/>
					<box name="default">
						<newPort name="flagIsUp" value="TRUE"/>
