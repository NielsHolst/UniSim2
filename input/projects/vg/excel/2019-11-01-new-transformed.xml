				<port name="endTime" externalName="Cli_MaxRelHmd" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/ToTime" value="23:59"/>
				<port name="signalInside" externalName="Cli_MaxRelHmd" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/Value" value="69.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="heatingTemperatureMargin">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
