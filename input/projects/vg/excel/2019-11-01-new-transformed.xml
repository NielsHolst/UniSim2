				<newPort name="signal" externalName="rhMaxBand" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="1.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="dawnThreshold">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="Cli_LightChangeFromNight" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="0.2"/>
			</box>
		</box>
		<box class="PrioritySignal" name="duskThreshold">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="Cli_LightChangeFromDay" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="0.3"/>
			</box>
		</box>
		<box class="PrioritySignal" name="crackVentilation">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="crackVentilation" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="5.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="crackVentilationTemperatureMin">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="crackVentilationTemperatureMin" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="-5.0"/>
			</box>
		</box>
