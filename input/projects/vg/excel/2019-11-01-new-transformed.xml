			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="screenEnergyThreshold1" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="5.0"/>
