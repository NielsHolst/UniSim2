					<port name="reverseOrder" value="TRUE"/>
					<box name="default">
						<newPort name="flagIsUp" value="TRUE"/>
						<newPort name="signal" externalName="AssLightOn3" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="40.0"/>
					</box>
				</box>
				<box class="PrioritySignal" name="thresholdHigh">
					<port name="reverseOrder" value="TRUE"/>
					<box name="default">
						<newPort name="flagIsUp" value="TRUE"/>
						<newPort name="signal" externalName="AssLightOff3" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="600.0"/>
