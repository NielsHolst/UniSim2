			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="crackVentilationTemperatureMinBand" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="1.0"/>
