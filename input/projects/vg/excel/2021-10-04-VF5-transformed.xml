		<port name="latitude" externalName="None" source="/DVV_SETUP/Latitude" value="55.4038"/>
		<port name="longitude" externalName="None" source="/DVV_SETUP/Longitude" value="10.4024"/>
	</box>
	<!-- *** Outdoors *** -->
