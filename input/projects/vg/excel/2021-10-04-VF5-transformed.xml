	<box class="Calendar" name="calendar">
		<port name="begin" externalName="StartTime" source="/DVV_SETUP/StartTime" compute="01-01-2021 - 1"/>
		<port name="end" externalName="StopTime" source="/DVV_SETUP/StopTime" value="07-01-2021"/>
		<port name="timeStep" externalName="TimeStep" source="/DVV_SETUP/TimeStep" value="3"/>
