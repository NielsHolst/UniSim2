<?xml version="1.0" encoding="ISO-8859-1"?>
<box xmlns:ecolmod="http://www.ecolmod.org" class="Simulation" name="greenhouse">
	<port name="steps" ref="./period[steps]"/>
	<!-- *** SimulationPeriod *** -->
	<box class="SimulationPeriod" name="period">
		<port name="beginDate" externalName="None" source="/DVV_SETUP/StartTime" value="01-01-2019"/>
		<port name="endDate" externalName="None" source="/DVV_SETUP/StopTime" value="07-01-2019"/>
		<port name="beginTime" externalName="None" source="Fixed" value="00:00:00"/>
		<port name="endTime" externalName="None" source="Fixed" value="00:00:00"/>
		<port name="timeStep" externalName="None" source="/DVV_SETUP/TimeStep" value="4"/>
		<port name="timeUnit" value="m" externalName="None" source="Fixed"/>
	</box>
	<!-- *** Calendar *** -->
	<box class="Calendar" name="calendar">
		<port name="initialDateTime" ref="../period[beginDateTime]"/>
		<port name="timeStep" ref="../period[timeStep]"/>
		<port name="timeUnit" ref="../period[timeUnit]"/>
		<port name="latitude" externalName="None" source="/DVV_SETUP/Latitude" value="55.5"/>
		<port name="longitude" externalName="None" source="/DVV_SETUP/Longitude" value="10.5"/>
	</box>
	<!-- *** Outdoors *** -->
	<box class="vg::Outdoors" name="outdoors">
		<box class="Records" name="records">
			<port name="fileName" externalName="None" source="/DVV_SETUP/Weather/File" value="../input/sel_dk.txt"/>
		</box>
	</box>
	<!-- *** Construction *** -->
	<box name="construction">
		<box class="vg::Geometry" name="geometry">
			<port name="orientation" externalName="Orientation" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="0"/>
			<port name="numSpans" externalName="NumberOfSpans" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="26"/>
			<port name="spanWidth" externalName="SpanSize" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="4"/>
			<port name="length" externalName="Length" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="100"/>
			<port name="height" externalName="TrempelHeight" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="4"/>
			<port name="roofPitch" externalName="RoofPitch" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="26"/>
			<port name="reflection" externalName="GreenhouseReductionFactorLight" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="0.2"/>
		</box>
		<box class="vg::Shelter" name="shelter">
			<box class="vg::ShelterFace" name="roof1">
				<box class="vg::Cover" name="cover">
					<port name="transmissivity" externalName="PaneTransmission" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="0.89"/>
					<port name="U4" externalName="PaneUValue" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="6.5"/>
				</box>
				<box class="vg::Screens" name="screens"/>
			</box>
			<box class="vg::ShelterFace" name="roof2">
				<box class="vg::Cover" name="cover">
					<port name="transmissivity" externalName="PaneTransmission" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="0.89"/>
					<port name="U4" externalName="PaneUValue" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="6.5"/>
				</box>
				<box class="vg::Screens" name="screens"/>
			</box>
			<box class="vg::ShelterFace" name="side1">
				<box class="vg::Cover" name="cover">
					<port name="transmissivity" externalName="PaneTransmission" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="0.89"/>
					<port name="U4" externalName="PaneUValue" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="6.5"/>
				</box>
				<box class="vg::Screens" name="screens"/>
			</box>
			<box class="vg::ShelterFace" name="side2">
				<box class="vg::Cover" name="cover">
					<port name="transmissivity" externalName="PaneTransmission" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="0.89"/>
					<port name="U4" externalName="PaneUValue" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="6.5"/>
				</box>
				<box class="vg::Screens" name="screens"/>
			</box>
			<box class="vg::ShelterFace" name="end1">
				<box class="vg::Cover" name="cover">
					<port name="transmissivity" externalName="PaneTransmission" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="0.89"/>
					<port name="U4" externalName="PaneUValue" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="6.5"/>
				</box>
				<box class="vg::Screens" name="screens"/>
			</box>
			<box class="vg::ShelterFace" name="end2">
				<box class="vg::Cover" name="cover">
					<port name="transmissivity" externalName="PaneTransmission" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="0.89"/>
					<port name="U4" externalName="PaneUValue" source="/DVV_SETUP/Greenhouse/Panes/Pane/Constants/Parameters/Value" value="6.5"/>
				</box>
				<box class="vg::Screens" name="screens"/>
			</box>
		</box>
	</box>
	<!-- *** Indoors *** -->
	<box class="vg::Indoors" name="indoors">
		<box class="vg::Given" name="given">
			<box class="vg::AirFluxGiven" name="airFlux">
				<box class="vg::AirFluxInfiltration" name="infiltration">
					<port name="leakage" externalName="leakage" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="0.1"/>
				</box>
			</box>
			<box class="vg::EnergyFluxSum" name="energyFlux">
				<box class="vg::EnergyFluxFloor" name="floor">
					<port name="Uindoors" externalName="floor-Uindoors" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="7.5"/>
					<port name="Usoil" externalName="floor-Usoil" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="4"/>
					<port name="heatCapacity" externalName="floor-heatCapacity" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="1000"/>
					<port name="emissivity" externalName="floor-emissivity" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="0.85"/>
					<box class="vg::FloorRadiationAbsorbed" name="radiationAbsorbed">
						<port name="reflectance" externalName="floor-reflectance" source="/DVV_SETUP/Greenhouse/Constants/Parameters/Value" value="0.5"/>
					</box>
				</box>
			</box>
		</box>
	</box>
	<!-- *** allSetpoints *** -->
	<box name="allSetpoints">
		<box class="PrioritySignal" name="heatingTemperatureAtLowRh">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="Cli_HeatingTemp" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="20.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="ventilationTemperatureMargin">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="Cli_VentTemp" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="5.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="rhMax">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="Cli_MaxRelHmd" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="68.0"/>
			</box>
			<box class="DateTimeSignal">
				<port name="beginDate" externalName="Cli_MaxRelHmd" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/FromDay" value="01-01-2019"/>
				<port name="endDate" externalName="Cli_MaxRelHmd" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/ToDay" value="31-12-2019"/>
				<port name="beginTime" externalName="Cli_MaxRelHmd" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/FromTime" value="18:00"/>
				<port name="endTime" externalName="Cli_MaxRelHmd" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/ToTime" value="23:59"/>
				<port name="signalInside" externalName="Cli_MaxRelHmd" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/Value" value="69.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="heatingTemperatureMargin">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="Cli_MaxHeatAddHighRH" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="2.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="ventilationTemperatureRhMargin">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="Cli_MaxVentDecHighRH" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="2.0"/>
			</box>
			<box class="DateTimeSignal">
				<port name="beginDate" externalName="Cli_MaxVentDecHighRH" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/FromDay" value="01-01-2019"/>
				<port name="endDate" externalName="Cli_MaxVentDecHighRH" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/ToDay" value="31-12-2019"/>
				<port name="beginTime" externalName="Cli_MaxVentDecHighRH" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/FromTime" value="18:00"/>
				<port name="endTime" externalName="Cli_MaxVentDecHighRH" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/ToTime" value="23:59"/>
				<port name="signalInside" externalName="Cli_MaxVentDecHighRH" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/Value" value="3.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="ventilationThresholdBand">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="ventilationsThresholdBand" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="1.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="co2Capacity">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="Cli_CO2Capacity" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="10.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="co2Setpoint">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="Cli_CO2Setpoint" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="900.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="co2VentilationThreshold">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="Cli_CO2VentilationThreshold" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="0.1"/>
			</box>
		</box>
		<box class="PrioritySignal" name="co2VentilationBand">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="Cli_CO2VentilationBand" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="0.2"/>
			</box>
		</box>
		<box class="PrioritySignal" name="chalk">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="Cli_ShadingAgentReduction" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="0.0"/>
			</box>
		</box>
		<box name="growthLights">
			<box name="growthLight1">
				<box class="PrioritySignal" name="thresholdLow">
					<port name="reverseOrder" value="TRUE"/>
					<box name="default">
						<newPort name="flagIsUp" value="TRUE"/>
						<newPort name="signal" externalName="AssLightOn1" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="40.0"/>
					</box>
				</box>
				<box class="PrioritySignal" name="thresholdHigh">
					<port name="reverseOrder" value="TRUE"/>
					<box name="default">
						<newPort name="flagIsUp" value="TRUE"/>
						<newPort name="signal" externalName="AssLightOff1" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="600.0"/>
					</box>
				</box>
				<box class="PrioritySignal" name="setting">
					<port name="reverseOrder" value="TRUE"/>
					<box name="default">
						<newPort name="flagIsUp" value="TRUE"/>
						<newPort name="signal" externalName="AssLightActive1" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="1.0"/>
					</box>
					<box class="DateTimeSignal">
						<port name="beginDate" externalName="AssLightActive1" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/FromDay" value="01-01-2019"/>
						<port name="endDate" externalName="AssLightActive1" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/ToDay" value="31-12-2019"/>
						<port name="beginTime" externalName="AssLightActive1" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/FromTime" value="18:00"/>
						<port name="endTime" externalName="AssLightActive1" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/ToTime" value="23:59"/>
						<port name="signalInside" externalName="AssLightActive1" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/ModSetpoint/Value" value="0.0"/>
					</box>
				</box>
			</box>
			<box name="growthLight2">
				<box class="PrioritySignal" name="thresholdLow">
					<port name="reverseOrder" value="TRUE"/>
					<box name="default">
						<newPort name="flagIsUp" value="TRUE"/>
						<newPort name="signal" externalName="AssLightOn2" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="40.0"/>
					</box>
				</box>
				<box class="PrioritySignal" name="thresholdHigh">
					<port name="reverseOrder" value="TRUE"/>
					<box name="default">
						<newPort name="flagIsUp" value="TRUE"/>
						<newPort name="signal" externalName="AssLightOff2" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="600.0"/>
					</box>
				</box>
				<box class="PrioritySignal" name="setting">
					<port name="reverseOrder" value="TRUE"/>
					<box name="default">
						<newPort name="flagIsUp" value="TRUE"/>
						<newPort name="signal" externalName="AssLightActive2" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="0.0"/>
					</box>
				</box>
			</box>
			<box name="growthLight3">
				<box class="PrioritySignal" name="thresholdLow">
					<port name="reverseOrder" value="TRUE"/>
					<box name="default">
						<newPort name="flagIsUp" value="TRUE"/>
						<newPort name="signal" externalName="AssLightOn3" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="40.0"/>
					</box>
				</box>
				<box class="PrioritySignal" name="thresholdHigh">
					<port name="reverseOrder" value="TRUE"/>
					<box name="default">
						<newPort name="flagIsUp" value="TRUE"/>
						<newPort name="signal" externalName="AssLightOff3" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="600.0"/>
					</box>
				</box>
				<box class="PrioritySignal" name="setting">
					<port name="reverseOrder" value="TRUE"/>
					<box name="default">
						<newPort name="flagIsUp" value="TRUE"/>
						<newPort name="signal" externalName="AssLightActive3" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="0.0"/>
					</box>
				</box>
			</box>
		</box>
		<box class="PrioritySignal" name="rhMaxBand">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="rhMaxBand" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="1.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="dawnThreshold">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="Cli_LightChangeFromNight" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="0.2"/>
			</box>
		</box>
		<box class="PrioritySignal" name="duskThreshold">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="Cli_LightChangeFromDay" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="0.3"/>
			</box>
		</box>
		<box class="PrioritySignal" name="crackVentilation">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="crackVentilation" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="5.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="crackVentilationTemperatureMin">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="crackVentilationTemperatureMin" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="-5.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="crackVentilationTemperatureMinBand">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="crackVentilationTemperatureMinBand" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="1.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="screenEnergyThreshold">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="screenEnergyThreshold" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="5.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="screenEnergyThresholdBand">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="screenEnergyThresholdBand" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="5.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="screenShadeThreshold">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="screenShadeThreshold" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="500.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="screenShadeThresholdBand">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="screenShadeThresholdBand" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="50.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="screenBlackoutFromTimeFloat">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="screenBlackoutFromTime" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="0.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="screenBlackoutToTimeFloat">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="screenBlackoutToTime" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="0.0"/>
			</box>
		</box>
		<box class="PrioritySignal" name="screenMaxAtHighRh">
			<port name="reverseOrder" value="TRUE"/>
			<box name="default">
				<newPort name="flagIsUp" value="TRUE"/>
				<newPort name="signal" externalName="screenMaxAtHighRh" source="/DVV_SETUP/Greenhouse/Climate/Setpoint/Constants/Parameters/Value" value="90.0"/>
			</box>
		</box>
	</box>
	<!-- *** Setpoints *** -->
	<box class="vg::Setpoints" name="setpoints">
		<port name="heatingTemperatureAtLowRh" ref="allSetpoints/heatingTemperatureAtLowRh[value]"/>
		<port name="heatingTemperatureMargin" ref="allSetpoints/heatingTemperatureMargin[value]"/>
		<port name="ventilationTemperatureMargin" ref="allSetpoints/ventilationTemperatureMargin[value]"/>
		<port name="ventilationTemperatureRhMargin" ref="allSetpoints/ventilationTemperatureRhMargin[value]"/>
		<port name="rhMax" ref="allSetpoints/rhMax[value]"/>
		<port name="rhMaxBand" ref="allSetpoints/rhMaxBand[value]"/>
		<port name="co2Capacity" ref="allSetpoints/co2Capacity[value]"/>
		<port name="co2Setpoint" ref="allSetpoints/co2Setpoint[value]"/>
		<port name="dawnThreshold" ref="allSetpoints/dawnThreshold[value]"/>
		<port name="duskThreshold" ref="allSetpoints/duskThreshold[value]"/>
	</box>
	<!-- *** Controllers *** -->
	<box class="vg::Controllers" name="controllers">
		<port name="ventilationThresholdBand" ref="allSetpoints/ventilationThresholdBand[value]"/>
		<port name="crackVentilation" ref="allSetpoints/crackVentilation[value]"/>
		<port name="crackVentilationTemperatureMin" ref="allSetpoints/crackVentilationTemperatureMin[value]"/>
		<port name="crackVentilationTemperatureMinBand" ref="allSetpoints/crackVentilationTemperatureMinBand[value]"/>
		<port name="screenMaxAtHighRh" ref="allSetpoints/screenMaxAtHighRh[value]"/>
		<port name="screenEnergyThreshold" ref="allSetpoints/screenEnergyThreshold[value]"/>
		<port name="screenEnergyThresholdBand" ref="allSetpoints/screenEnergyThresholdBand[value]"/>
		<port name="screenShadeThreshold" ref="allSetpoints/screenShadeThreshold[value]"/>
		<port name="screenShadeThresholdBand" ref="allSetpoints/screenShadeThresholdBand[value]"/>
		<port name="screenBlackoutFromTimeFloat" ref="allSetpoints/screenBlackoutFromTimeFloat[value]"/>
		<port name="screenBlackoutToTimeFloat" ref="allSetpoints/screenBlackoutToTimeFloat[value]"/>
		<port name="chalk" ref="allSetpoints/chalk[value]"/>
		<box name="growthLights">
			<box class="GrowthLightController" name="growthLight1">
				<port name="setting" ref="allSetpoints/growthLights/growthLight1/setting[value]"/>
				<port name="lightThresholdLow" ref="allSetpoints/growthLights/growthLight1/thresholdLow[value]"/>
				<port name="lightThresholdHigh" ref="allSetpoints/growthLights/growthLight1/thresholdHigh[value]"/>
			</box>
			<box class="GrowthLightController" name="growthLight2">
				<port name="setting" ref="allSetpoints/growthLights/growthLight2/setting[value]"/>
				<port name="lightThresholdLow" ref="allSetpoints/growthLights/growthLight2/thresholdLow[value]"/>
				<port name="lightThresholdHigh" ref="allSetpoints/growthLights/growthLight2/thresholdHigh[value]"/>
			</box>
			<box class="GrowthLightController" name="growthLight3">
				<port name="setting" ref="allSetpoints/growthLights/growthLight3/setting[value]"/>
				<port name="lightThresholdLow" ref="allSetpoints/growthLights/growthLight3/thresholdLow[value]"/>
				<port name="lightThresholdHigh" ref="allSetpoints/growthLights/growthLight3/thresholdHigh[value]"/>
			</box>
		</box>
	</box>
	<!-- *** Actuators *** -->
	<box class="vg::Actuators" name="actuators">
		<box class="GrowthLights" name="growthLights">
			<box class="vg::GrowthLight" name="growthLight1">
				<port name="on" ref="controllers/growthLights/growthLight1[flagIsUp]"/>
				<port name="intensity" externalName="LightCapacityPerSqm" source="/DVV_SETUP/Greenhouse/Lamps/Lamp/Constants/Parameters/Value" value="50"/>
				<port name="parPhotonCoef" externalName="MicromolParPerWatt" source="/DVV_SETUP/Greenhouse/Lamps/Lamp/Constants/Parameters/Value" value="1.9"/>
				<port name="ageCorrectedEfficiency" externalName="ageCorrectedEfficiency" source="/DVV_SETUP/Greenhouse/Lamps/Lamp/Constants/Parameters/Value" value="1"/>
			</box>
			<box class="vg::GrowthLight" name="growthLight2">
				<port name="on" ref="controllers/growthLights/growthLight2[flagIsUp]"/>
				<port name="intensity" externalName="LightCapacityPerSqm" source="/DVV_SETUP/Greenhouse/Lamps/Lamp/Constants/Parameters/Value" value="40"/>
				<port name="parPhotonCoef" externalName="MicromolParPerWatt" source="/DVV_SETUP/Greenhouse/Lamps/Lamp/Constants/Parameters/Value" value="0"/>
				<port name="ageCorrectedEfficiency" externalName="ageCorrectedEfficiency" source="/DVV_SETUP/Greenhouse/Lamps/Lamp/Constants/Parameters/Value" value="1"/>
			</box>
			<box class="vg::GrowthLight" name="growthLight3">
				<port name="on" ref="controllers/growthLights/growthLight3[flagIsUp]"/>
				<port name="intensity" externalName="LightCapacityPerSqm" source="/DVV_SETUP/Greenhouse/Lamps/Lamp/Constants/Parameters/Value" value="30"/>
				<port name="parPhotonCoef" externalName="MicromolParPerWatt" source="/DVV_SETUP/Greenhouse/Lamps/Lamp/Constants/Parameters/Value" value="0"/>
				<port name="ageCorrectedEfficiency" externalName="ageCorrectedEfficiency" source="/DVV_SETUP/Greenhouse/Lamps/Lamp/Constants/Parameters/Value" value="1"/>
			</box>
		</box>
		<box class="ProportionalSignal" name="heating">
			<port name="initialSignal" ref="indoors/temperature[value]"/>
			<port name="input" ref="controllers/heating[value]"/>
			<port name="threshold" value="0"/>
			<port name="thresholdBand" value="1"/>
			<port name="minSignal" ref="indoors/temperature[value]"/>
			<port name="maxSignal" externalName="None" source="Fixed" value="60"/>
			<box name="pipes">
				<box class="vg::Pipe" name="pipe">
					<port name="density" externalName="PipeLengthPerSqm" source="/DVV_SETUP/Greenhouse/Heatpipes/Heatpipe/Constants/Parameters/Value" value="1.8"/>
					<port name="diameter" externalName="InnerDiameter" source="/DVV_SETUP/Greenhouse/Heatpipes/Heatpipe/Constants/Parameters/Value" value="16"/>
					<port name="flowRate" externalName="heatPipeFlowRate" source="/DVV_SETUP/Greenhouse/Heatpipes/Heatpipe/Constants/Parameters/Value" value="2"/>
					<port name="minTemperature" externalName="CommonReturnTemperature" source="/DVV_SETUP/Greenhouse/Heatpipes/Heatpipe/Constants/Parameters/Value" value="20"/>
					<port name="maxTemperature" externalName="CommonFlowTemperature" source="/DVV_SETUP/Greenhouse/Heatpipes/Heatpipe/Constants/Parameters/Value" value="80"/>
				</box>
				<box class="vg::Pipe" name="pipe">
					<port name="density" externalName="PipeLengthPerSqm" source="/DVV_SETUP/Greenhouse/Heatpipes/Heatpipe/Constants/Parameters/Value" value="1.8"/>
					<port name="diameter" externalName="InnerDiameter" source="/DVV_SETUP/Greenhouse/Heatpipes/Heatpipe/Constants/Parameters/Value" value="16"/>
					<port name="flowRate" externalName="heatPipeFlowRate" source="/DVV_SETUP/Greenhouse/Heatpipes/Heatpipe/Constants/Parameters/Value" value="2"/>
					<port name="minTemperature" externalName="CommonReturnTemperature" source="/DVV_SETUP/Greenhouse/Heatpipes/Heatpipe/Constants/Parameters/Value" value="20"/>
					<port name="maxTemperature" externalName="CommonFlowTemperature" source="/DVV_SETUP/Greenhouse/Heatpipes/Heatpipe/Constants/Parameters/Value" value="80"/>
				</box>
			</box>
		</box>
	</box>
	<!-- *** Crop *** -->
	<box class="vg::Crop" name="crop">
		<box name="lai">
			<newPort name="value" externalName="LAI" source="/DVV_SETUP/Greenhouse/Crop/Parameters/Value" value="1"/>
			<newPort name="fractionPlantArea" externalName="propAreaCultured" source="/DVV_SETUP/Greenhouse/Crop/Parameters/Value" value="0.95"/>
		</box>
	</box>
	<box class="vg::Budget" name="budget"/>
	<!-- *** Output *** -->
	<box class="OutputR" name="output">
		<box name="p">
			<newPort name="outdoorsCo2" ref="outdoors[co2]"/>
