Simulation sim {
	.steps = 100
	Sequence gmc {
		.min = 5
		.max = 25
	}
	ExponentialPeak devRateGmc {
		.x = gmc[value]
		.optimum = 15.14
		.d1 = 5.169
		.d2 = 16.42
	}
	ExponentialPeak survivalGmc {
		.x = gmc[value]
		.optimum = 15.10
		.d1 = 5.258
		.d2 = 19.58
	}
	OutputR {
    PageR {
			.xAxis = gmc[value]
      PlotR {
				.layout = "merged"
        .ports = (devRateGmc[value] survivalGmc[value])
      }
		}
	}
}

