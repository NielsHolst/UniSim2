Simulation sim {
  .iterations = 35
  .steps = 2
  Box random {
    RandomiserMonteCarlo randomiser {
      .useFixed = FALSE
      .doSensitivityAnalysis = TRUE
    }
    RandomBinomial binomial {
      .P = 0.8
    }
    RandomLognormal lognormal {
      .min = 100
      .max = 10000
      .P = 0.95
    }
    RandomNormal normal {
      .min = 20
      .max = 40
      .P = 0.95
    }
    RandomUniform uniform {
      .min = 20
      .max = 40
    }
    RandomUniformInt uniformInt {
      .min = 20
      .max = 40
    }
  }
  OutputR {
    .end = "random-end.R"
    .keepPages = TRUE
    OutputText {
      .ports = random/*[value]
    }
  }
}