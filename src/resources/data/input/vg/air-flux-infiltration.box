Simulation {
	.iterations = 30
	.steps = 5
	SensitivityAnalysis {
	}
	AirFluxInfiltration infiltration {
		.leakage = 1 @(uniform 0.5 3)
		.windSpeed = 4 @(uniform 0 25)
		.airTransmissivity = 1
	}
	OutputR {
		PageR {
			.xAxis = *<Distribution>/..<Port>
			PlotR {
				.ports = infiltration[value]|end
				.ggplot = "geom_point() + geom_smooth(method='lm') + ylim(0,NA)"
			}
		}
	}
}