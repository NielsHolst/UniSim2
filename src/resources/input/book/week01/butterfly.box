Simulation sim {
	.steps = 70
	Box butterfly {
		Stage egg {
			.initial = 100 
			.duration = 7
		}
		Stage larva {
			.inflow = ../egg[outflow]
			.duration = 15
		}
		Stage pupa {
			.inflow = ../larva[outflow]
			.duration = 7
		}
		Stage adult {
			.inflow = ../pupa[outflow]
			.duration = 30
		}
	}
	OutputR {
		.clear = TRUE
    PageR {
      .xAxis = sim[step]
      PlotR {
        .ports = (butterfly/*[content])
      }
		}
	}
}

